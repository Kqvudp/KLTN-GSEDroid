<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="eng.trickortracker.us">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar">
        <uses-library android:name="com.google.android.maps"/>
        <activity android:label="@string/app_name" android:name=".Tricker">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Settings" android:screenOrientation="portrait" android:windowSoftInputMode="adjustPan"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".AlertAudios" android:screenOrientation="portrait" android:theme="@android:style/Theme.Dialog"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".AlertDialogs" android:screenOrientation="portrait" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name=".Registration" android:screenOrientation="portrait"/>
        <activity android:name=".DisplayMap" android:screenOrientation="portrait"/>
        <activity android:name=".DisplayMap1" android:screenOrientation="portrait"/>
        <activity android:name=".Expired" android:screenOrientation="portrait"/>
        <activity android:name=".ChildTricker" android:screenOrientation="portrait"/>
        <activity android:name=".ParentTricker" android:screenOrientation="portrait"/>
        <activity android:name=".ClientSettings" android:screenOrientation="portrait" android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".GlowSticks" android:screenOrientation="portrait"/>
        <service android:enabled="true" android:exported="false" android:name=".SendMsg"/>
        <service android:enabled="true" android:exported="false" android:name=".MsgPro"/>
        <service android:enabled="true" android:exported="false" android:name=".MySendMsg"/>
        <service android:enabled="true" android:exported="false" android:name=".OpenDial"/>
        <service android:enabled="true" android:exported="false" android:name=".OpenDial1"/>
        <service android:enabled="true" android:exported="false" android:name=".Neram"/>
        <service android:enabled="true" android:exported="false" android:name=".GetLocation"/>
        <receiver android:name=".SmsReceiver">
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_RECEIVED"/>
            </intent-filter>
        </receiver>
    </application>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
</manifest>