        <receiver android:name="NoteWidgetProvider">
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/notewidget_provider"/>
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
            </intent-filter>
        </receiver>
        <receiver android:name="com.cjm.gogoNote.NoteAlarmReceiver">
            <intent-filter>
                <action android:name="com.android.broadcasttest.TIME_ALERT"/>
            </intent-filter>
        </receiver>
        <activity android:name="NoteWidgetConfigure" android:screenOrientation="portrait">
