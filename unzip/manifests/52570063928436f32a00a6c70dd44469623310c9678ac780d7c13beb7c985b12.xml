<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.batteryupgradeandroid.batteryupdate.killer">
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name="com.bwx.bequick.SettingsApplication">
        <activity android:configChanges="keyboardHidden" android:label="@string/app_name" android:launchMode="singleInstance" android:name="com.savebattery.killer.pro.EasyTaskKiller">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PICK_SAVE_BATTERY_KILLER_PRO"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.savebattery.killer.pro.IgnoreList">
            <intent-filter>
                <action android:name="android.intent.action.SAVE_BATTERY_PRO_IGNORE_LIST"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.savebattery.killer.pro.AddToIgnoreList"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name="com.savebattery.killer.pro.PreferencesActivity">
            <intent-filter>
                <action android:name="android.intent.action.SAVE_BATTERY_SETTINGS"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <service android:name="com.savebattery.killer.pro.AutoKillService">
            <intent-filter>
                <action android:name="com.savebattery.killer.pro.action.AUTO_KILL"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </service>
        <receiver android:name="com.savebattery.killer.pro.AutoStartReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </receiver>
        <receiver android:name="com.savebattery.killer.pro.widget.EasyWidgetProvider">
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/appwidget_provider"/>
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
            </intent-filter>
        </receiver>
        <service android:name="com.savebattery.killer.pro.widget.WidgetUpdateService"/>
        <receiver android:name="com.savebattery.killer.pro.widget.KillAllEventReceiver">
            <intent-filter>
                <action android:name="com.savebattery.killer.pro.action.KILLALL"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </receiver>
        <activity android:icon="@drawable/ic_logo" android:label="Quick Settings" android:launchMode="standard" android:name="com.bwx.bequick.EulaActivity" android:theme="@android:style/Theme.NoDisplay"/>
        <activity android:icon="@drawable/ic_logo" android:label="Quick Settings" android:launchMode="standard" android:name="com.bwx.bequick.ShowSettingsActivity" android:theme="@android:style/Theme.NoDisplay">
            <intent-filter>
                <action android:name="android.intent.action.SEARCH_LONG_PRESS"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:excludeFromRecents="true" android:icon="@drawable/ic_logo" android:label="Quick Settings" android:launchMode="singleInstance" android:name="com.bwx.bequick.DialogSettingsActivity" android:theme="@style/Theme.NoTitleDialog"/>
        <activity android:excludeFromRecents="true" android:icon="@drawable/ic_logo" android:label="Quick Settings" android:launchMode="singleInstance" android:name="com.bwx.bequick.MainSettingsActivity" android:theme="@style/Theme.NoTitle"/>
        <activity android:excludeFromRecents="true" android:label="@string/txt_customize_settings" android:name="com.bwx.bequick.LayoutSettingsActivity" android:theme="@style/Theme.NoTitle"/>
        <activity android:excludeFromRecents="true" android:label="@string/txt_preferences" android:name="com.bwx.bequick.preferences.CommonPrefs"/>
        <activity android:excludeFromRecents="true" android:label="@string/txt_preferences" android:name="com.bwx.bequick.preferences.BrightnessPrefs"/>
        <activity android:excludeFromRecents="true" android:label="@string/txt_preferences" android:name="com.bwx.bequick.preferences.MobileDataPrefs"/>
        <activity android:excludeFromRecents="true" android:label="@string/txt_preferences" android:name="com.bwx.bequick.preferences.AirplaneModePrefs"/>
        <activity android:excludeFromRecents="true" android:finishOnTaskLaunch="true" android:label="Flashlight" android:launchMode="singleInstance" android:name="com.bwx.bequick.flashlight.ScreenLightActivity" android:noHistory="true" android:screenOrientation="portrait"/>
        <receiver android:name="com.bwx.bequick.flashlight.LedFlashlightReceiver">
            <intent-filter>
                <action android:name="com.bwx.bequick.FLASHLIGHT"/>
                <action android:name="com.bwx.bequick.WARN_FLASHLIGHT"/>
            </intent-filter>
        </receiver>
        <receiver android:name="com.bwx.bequick.receivers.StatusBarIntegrationReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
                <action android:name="com.bwx.bequick.UPDATE_STATUSBAR_INTEGRATION"/>
                <action android:name="com.bwx.bequick.INIT_STATUSBAR_INTEGRATION"/>
                <action android:name="com.bwx.bequick.START_QS"/>
            </intent-filter>
        </receiver>
        <activity android:name="com.savebattery.killer.pro.OfferDialog"/>
        <receiver android:exported="true" android:name="com.savebattery.killer.pro.BaseStarReceiver">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"/>
            </intent-filter>
        </receiver>
    </application>
    <supports-screens android:anyDensity="true" android:largeScreens="true" android:normalScreens="true" android:smallScreens="true"/>
    <uses-permission android:name="android.permission.RESTART_PACKAGES"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.WRITE_APN_SETTINGS"/>
    <uses-permission android:name="android.permission.READ_SYNC_SETTINGS"/>
    <uses-permission android:name="android.permission.WRITE_SYNC_SETTINGS"/>
    <uses-permission android:name="android.permission.GET_ACCOUNTS"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.FLASHLIGHT" android:required="false"/>
    <uses-permission android:name="android.permission.HARDWARE_TEST" android:required="false"/>
    <uses-permission android:name="android.permission.WRITE_SECURE_SETTINGS" android:required="false"/>
    <uses-permission android:name="android.permission.READ_SECURE_SETTINGS" android:required="false"/>
    <uses-permission android:name="android.permission.CAMERA" android:required="false"/>
    <uses-permission android:name="android.permission.MODIFY_PHONE_STATE"/>
</manifest>