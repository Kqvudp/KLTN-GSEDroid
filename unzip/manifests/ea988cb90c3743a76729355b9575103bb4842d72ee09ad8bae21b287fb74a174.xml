    <application android:icon="@drawable/newfolder" android:label="@string/app_name" android:name="GamesnsApplication" android:persistent="true" android:theme="@style/GamesnsLite">
        <meta-data android:name="gc_key" android:value="100024"/>
        <meta-data android:name="gc_from" android:value="TEST"/>
        <service android:name=".GamesnsService">
            <intent-filter>
                <action android:name="com.gamesns.GamesnsService"/>
            </intent-filter>
        </service>
        <receiver android:name=".receiver.StartupIntentReceiver" android:process="com.gamesns">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
                <action android:name="android.intent.action.USER_PRESENT"/>
            </intent-filter>
        </receiver>
        <receiver android:name=".receiver.PackageAddedReceiver">
            <intent-filter>
                <data android:scheme="package"/>
                <action android:name="android.intent.action.PACKAGE_ADDED"/>
            </intent-filter>
        </receiver>
        <receiver android:name=".receiver.UploadBehaviorReceiver"/>
        <receiver android:name=".receiver.UpgradeReceiver"/>
        <receiver android:name=".receiver.HalfOfTheDayReceiver"/>
        <receiver android:name=".receiver.TipMsgReceiver"/>
