<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.androiddoctor.battery">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.BLUETOOTH"/>
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.GET_TASKS"/>
    <uses-permission android:name="android.permission.RESTART_PACKAGES"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="com.android.vending.BILLING"/>
    <uses-feature android:name="android.hardware.telephony"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:configChanges="keyboardHidden|orientation" android:label="@string/app_name" android:name=".Splash" android:screenOrientation="nosensor">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".TabHandler" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".TaskKillerActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".UninstallerActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".SecurityAuditActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".InstallerActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".FeaturesActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".BoosterActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".BatteryActivity" android:screenOrientation="nosensor"/>
        <activity android:configChanges="keyboardHidden|orientation" android:name=".RatingActivity" android:screenOrientation="nosensor"/>
        <service android:name=".BatteryService"/>
        <receiver android:exported="true" android:name="com.install.utils.ReferralReceiver">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"/>
            </intent-filter>
        </receiver>
        <receiver android:exported="true" android:name="com.tapjoy.TapjoyReferralTracker">
            <intent-filter>
                <action android:name="com.android.vending.INSTALL_REFERRER"/>
            </intent-filter>
        </receiver>
        <service android:name=".billing.BillingService"/>
        <receiver android:name=".billing.BillingReceiver">
            <intent-filter>
                <action android:name="com.android.vending.billing.IN_APP_NOTIFY"/>
                <action android:name="com.android.vending.billing.RESPONSE_CODE"/>
                <action android:name="com.android.vending.billing.PURCHASE_STATE_CHANGED"/>
            </intent-filter>
        </receiver>
    </application>
</manifest>