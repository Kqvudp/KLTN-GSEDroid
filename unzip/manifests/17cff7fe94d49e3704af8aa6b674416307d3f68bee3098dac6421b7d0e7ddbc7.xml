<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="net.kidlogger.kidloggerkeyboard">
    <application android:icon="@drawable/logging" android:label="@string/ime_name">
        <service android:name=".SoftKeyboardPRO" android:permission="android.permission.BIND_INPUT_METHOD">
            <intent-filter>
                <action android:name="android.view.InputMethod"/>
            </intent-filter>
            <meta-data android:name="android.view.im" android:resource="@xml/method"/>
        </service>
        <activity android:label="Settings" android:name=".Settings" android:windowSoftInputMode="adjustPan"/>
        <activity android:name="net.kidlogger.kidloggerkeyboard.InputLanguageSelection">
            <intent-filter>
                <action android:name="net.kidlogger.kidloggerkeyboard.INPUT_LANGUAGE_SELECTION"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
    </application>
</manifest>