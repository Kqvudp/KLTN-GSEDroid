<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.sivartech.SpyPhoneLite">
    <application android:debuggable="false" android:icon="@drawable/icon" android:label="@string/app_name" android:name="SpyPhoneApplication">
        <activity android:label="@string/app_name" android:name=".a_camera" android:screenOrientation="user" android:theme="@style/MyTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="a_audio" android:screenOrientation="portrait" android:theme="@style/MyTheme"/>
        <activity android:label="@string/app_name" android:name="a_video" android:screenOrientation="user" android:theme="@style/MyTheme"/>
        <activity android:label="@string/app_name" android:name="a_videoFroyo" android:screenOrientation="portrait" android:theme="@style/MyTheme"/>
        <activity android:name="com.sivartech.SpyPhoneBase.a_audio"/>
        <activity android:name="com.sivartech.SpyPhoneBase.a_camera" android:screenOrientation="user"/>
        <activity android:name="com.sivartech.SpyPhoneBase.a_videoFroyo"/>
        <activity android:name="com.sivartech.SpyPhoneBase.a_video"/>
        <activity android:label="@string/app_name" android:name="com.sivartech.SpyPhoneBase.Preferences" android:theme="@style/MyTheme"/>
        <activity android:configChanges="keyboard|keyboardHidden|orientation" android:name="com.google.ads.AdActivity"/>
    </application>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
</manifest>