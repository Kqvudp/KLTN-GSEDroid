<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="preferExternal" package="net.skyscanner.android">
    <uses-feature android:name="android.hardware.location" android:required="false"/>
    <uses-feature android:name="android.hardware.telephony" android:required="false"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name="net.skyscanner.android.SkyscannerApplication" android:theme="@style/SkyscannerTheme">
        <activity android:name="net.skyscanner.android.activity.SearchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <supports-screens android:anyDensity="true" android:largeScreens="true" android:normalScreens="true" android:smallScreens="true"/>
        <activity android:name="net.skyscanner.android.activity.DestinationSearchActivity" android:windowSoftInputMode="adjustPan|stateUnchanged"/>
        <activity android:name="net.skyscanner.android.activity.DatePickerActivity"/>
        <activity android:name="net.skyscanner.android.activity.PassengerPickerActivity"/>
        <activity android:name="net.skyscanner.android.activity.SearchResultsActivity"/>
        <activity android:name="net.skyscanner.android.activity.FilterResultsActivity"/>
        <activity android:name="net.skyscanner.android.activity.filter.StopsFilterActivity"/>
        <activity android:name="net.skyscanner.android.activity.filter.AirportFilterActivity"/>
        <activity android:name="net.skyscanner.android.activity.filter.AirlineFilterActivity"/>
        <activity android:name="net.skyscanner.android.activity.filter.TimeOfDayFilterActivity"/>
        <activity android:name="net.skyscanner.android.activity.filter.DurationFilterActivity"/>
        <activity android:name="net.skyscanner.android.activity.journey.JourneyResultActivity"/>
        <activity android:name="net.skyscanner.android.activity.MultipleBookingsActivity"/>
        <activity android:name="net.skyscanner.android.activity.InformationActivity"/>
        <activity android:name="net.skyscanner.android.activity.SettingsActivity"/>
    </application>
</manifest>