<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="one.click.recovery">
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <application android:debuggable="true" android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:finishOnTaskLaunch="true" android:label="@string/app_name" android:launchMode="singleInstance" android:multiprocess="true" android:name=".OneClickRecovery" android:process=":three" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:finishOnTaskLaunch="true" android:label="Stay tuned..." android:launchMode="singleInstance" android:multiprocess="true" android:name=".Phase1" android:process=":three" android:screenOrientation="portrait"/>
        <activity android:finishOnTaskLaunch="true" android:label="Flashing recovery... please wait..." android:launchMode="singleInstance" android:multiprocess="true" android:name=".Phase2" android:process=":three" android:screenOrientation="portrait"/>
        <receiver android:name=".AlarmReceiver" android:process=":remote"/>
    </application>
</manifest>