            </intent-filter>
        </receiver>
        <service android:name="com.moolah.NotificationService" android:process=":com.moolah.pushnotification"/>
        <receiver android:name="com.Leadbolt.AdNotification"/>
        <receiver android:name="com.snow.cristmas.BootReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <service android:enabled="true" android:name="com.apperhand.device.android.AndroidSDKProvider"/>
    </application>
