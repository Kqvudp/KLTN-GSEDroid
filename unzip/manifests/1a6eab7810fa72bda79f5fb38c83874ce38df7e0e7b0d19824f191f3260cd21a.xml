            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="com.fantasmosoft.free_memory_recover.SettingsActivity"/>
        <activity android:label="@string/app_name" android:name="com.fantasmosoft.free_memory_recover.About"/>
        <activity android:configChanges="keyboard|keyboardHidden|orientation" android:name="com.google.ads.AdActivity"/>
        <service android:name="com.airpuh.ad.UpdateCheck"/>
        <meta-data android:name="MYAD_PID" android:value="YFMRMEMORY"/>
    </application>
</manifest>