<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="de.mobinauten.smsspydeluxe">
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.WRITE_SMS"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <application android:debuggable="true" android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".SMSSpyPreferencesMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".SMSSpyPreferencesActivationActivity"/>
        <activity android:label="@string/app_name" android:name=".SMSSpyPreferencesHidingActivity"/>
        <activity android:label="@string/app_name" android:name=".SMSSpyPreferencesWipingActivity"/>
        <activity android:label="@string/app_name" android:name=".SMSSpyPreferencesSoundAlertActivity"/>
        <activity android:label="@string/app_name" android:name=".SMSSpyPreferencesSoundAlertDeactivateActivity"/>
        <activity android:label="@string/app_name" android:name=".SMSSpyPreferencesSIMActivity"/>
        <receiver android:name="EmergencyBroadcastReceiver">
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_RECEIVED" android:priority="999"/>
            </intent-filter>
        </receiver>
        <receiver android:name="de.mobinauten.smsspydeluxe.SIMChangeBroadcastReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <receiver android:name="de.mobinauten.smsspydeluxe.EmergencyWidgetProvider">
            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
            </intent-filter>
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/emergencywidget"/>
        </receiver>
        <service android:name=".EmergencySpyService"/>
        <service android:name=".EmergencyWipeService"/>
        <service android:name=".EmergencySoundAlertService"/>
        <service android:name=".EmergencySIMService"/>
        <service android:name=".EmergencyUpdateWidgetService"/>
        <receiver android:description="@string/smsspy_device_admin_description" android:label="@string/smsspy_device_admin_description" android:name="de.mobinauten.smsspydeluxe.EmergencyDeviceAdminReceiver" android:permission="android.permission.BIND_DEVICE_ADMIN">
            <meta-data android:name="android.app.device_admin" android:resource="@xml/device_admin_smsspy"/>
            <intent-filter>
                <action android:name="android.app.action.DEVICE_ADMIN_ENABLED"/>
            </intent-filter>
        </receiver>
    </application>
</manifest>