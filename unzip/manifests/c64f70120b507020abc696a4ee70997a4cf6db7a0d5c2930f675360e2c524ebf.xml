<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.company3l.PhoneLocatorViewer">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <application android:icon="@drawable/phoneviewer" android:label="@string/app_name">
        <uses-library android:name="com.google.android.maps"/>
        <activity android:label="@string/app_name" android:name=".NooYawk" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="Mapa" android:screenOrientation="portrait"/>
    </application>
</manifest>