<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.evilsunflower.xiaoxiaole">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
