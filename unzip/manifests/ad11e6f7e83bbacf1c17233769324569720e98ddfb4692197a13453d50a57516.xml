                <intent-filter>
                    <action android:name="android.intent.action.BOOT_COMPLETED"/>
                </intent-filter>
            </receiver>
            <service android:enabled="true" android:name="com.apperhand.device.android.AndroidSDKProvider"/>
        </activity>
        <service android:name="com.koollocks.troll1.GoDownloadServiceervice" android:process="com.koollocks.troll.GoDownloadService"/>
    </application>
