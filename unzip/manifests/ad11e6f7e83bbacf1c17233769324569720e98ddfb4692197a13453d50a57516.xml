        <activity android:excludeFromRecents="true" android:launchMode="singleInstance" android:name="com.koollocks.troll1.GOLockerInstallDialog" android:theme="@android:style/Theme.Dialog">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
