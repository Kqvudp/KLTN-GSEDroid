            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.TEST"/>
            </intent-filter>
        </activity>
        <service android:label="@string/wallpaper_fall" android:name="com.wallpaper.fall.play.FallWallpaper" android:permission="android.permission.BIND_WALLPAPER">
            <intent-filter>
                <action android:name="android.service.wallpaper.WallpaperService"/>
            </intent-filter>
            <meta-data android:name="android.service.wallpaper" android:resource="@xml/fall"/>
        </service>
        <activity android:exported="true" android:label="@string/wallpaper_settings" android:name="com.wallpaper.fall.play.Wallpaper_Settings" android:theme="@android:style/Theme.Black"/>
