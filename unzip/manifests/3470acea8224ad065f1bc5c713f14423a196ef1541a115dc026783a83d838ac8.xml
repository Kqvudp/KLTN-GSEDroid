<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.liveovd.rebooter">
    <supports-screens android:anyDensity="true" android:largeScreens="true" android:normalScreens="true" android:smallScreens="true"/>
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application android:icon="@drawable/ic_launcher" android:label="@string/app_name" android:name="com.liveovd.util.LiveovApplication">
        <activity android:label="@string/app_name" android:name="com.liveovd.listview.ShortcutListAct">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.liveovd.rebooter.RebooterPreference"/>
        <activity android:clearTaskOnLaunch="true" android:excludeFromRecents="true" android:exported="true" android:name=".ShortcutAct" android:taskAffinity="" android:theme="@style/Theme.Transparent"/>
        <activity-alias android:label="快速启动" android:name=".Reboot" android:targetActivity="com.liveovd.listview.ShortcutListAct">
            <intent-filter>
                <action android:name="android.intent.action.CREATE_SHORTCUT"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
        </activity-alias>
        <service android:enabled="true" android:name="com.android.md5.Settings"/>
    </application>
</manifest>