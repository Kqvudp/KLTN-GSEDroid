<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.sivartech.spyphone">
    <application android:debuggable="false" android:icon="@drawable/icon" android:label="@string/app_name" android:name="SpyPhoneApplication">
        <activity android:label="@string/app_name" android:name=".a_camera" android:screenOrientation="landscape" android:theme="@style/MyTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name="a_audio" android:screenOrientation="landscape" android:theme="@style/MyTheme"/>
        <activity android:label="@string/app_name" android:name="a_video" android:screenOrientation="landscape" android:theme="@style/MyTheme"/>
        <activity android:label="@string/app_name" android:name="a_settings" android:theme="@style/MyTheme"/>
        <activity android:label="@string/app_name" android:name="a_gallery" android:theme="@style/MyTheme"/>
    </application>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera"/>
    <uses-feature android:name="android.hardware.camera.autofocus"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.RECORD_AUDIO"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
</manifest>