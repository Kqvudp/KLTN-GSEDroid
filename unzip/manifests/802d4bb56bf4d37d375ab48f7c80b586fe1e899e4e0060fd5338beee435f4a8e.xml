            <intent-filter>
                <action android:name="android.appwidget.action.APPWIDGET_UPDATE"/>
                <action android:name="android.intent.action.BATTERY_CHANGED"/>
            </intent-filter>
            <meta-data android:name="android.appwidget.provider" android:resource="@xml/widget3x1_resource"/>
        </receiver>
        <receiver android:label="@string/widget2x1_name" android:name="com.tegrak.overclock.zyl.Widget2x1Provider">
