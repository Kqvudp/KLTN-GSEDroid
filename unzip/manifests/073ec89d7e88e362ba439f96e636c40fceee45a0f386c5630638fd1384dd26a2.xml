<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.anydown.wjbb">
    <application android:icon="@drawable/apklogo" android:label="@string/app_name" android:name=".DDStoreApplication">
        <activity android:configChanges="keyboardHidden|navigation|orientation" android:label="@string/app_name" android:name=".HomePageActivity" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:configChanges="keyboardHidden|navigation|orientation" android:launchMode="singleTask" android:name=".MainActivity"/>
        <activity android:configChanges="keyboardHidden|navigation|orientation" android:name=".RankChildActivity"/>
        <activity android:configChanges="keyboardHidden|navigation|orientation" android:name=".SearchActivity" android:windowSoftInputMode="adjustPan"/>
        <activity android:configChanges="keyboardHidden|navigation|orientation" android:name=".AppInfoListActivity"/>
        <activity android:configChanges="keyboardHidden|navigation|orientation" android:launchMode="singleTask" android:name=".MyDownloadActivity"/>
        <activity android:configChanges="keyboardHidden|navigation|orientation" android:name=".SettingsActivity"/>
        <activity android:name=".InstallPackageActivity">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT"/>
                <action android:name="com.anydown.wjbb.SYSINSTALL_APK"/>
                <data android:scheme="file"/>
                <data android:mimeType="application/vnd.android.package-archive"/>
            </intent-filter>
        </activity>
        <service android:label="DownloadService" android:name=".download.DownloadService"/>
        <service android:label="PromotionService" android:name=".download.PromotionService"/>
        <service android:label="UpdateAllService" android:name=".download.UpdateAllService"/>
        <receiver android:name=".receiver.InstallReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.anydown.wjbb.SYSINSTALL_APK"/>
                <data android:host="com.anydown.wjbb.contents" android:mimeType="vnd.android.cursor.item/com.anydown.wjbb.contents" android:scheme="content"/>
            </intent-filter>
            <intent-filter>
                <action android:name="com.anydown.wjbb.UNINSTALL_APK"/>
                <data android:host="com.anydown.wjbb.contents" android:mimeType="vnd.android.cursor.item/com.anydown.wjbb.contents" android:scheme="content"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_ADDED"/>
                <data android:scheme="package"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED"/>
                <data android:scheme="package"/>
            </intent-filter>
        </receiver>
        <receiver android:name=".receiver.LauncherReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <provider android:authorities="com.anydown.wjbb.contents" android:name=".provider.MarketProvider"/>
        <provider android:authorities="com.anydown.wjbb.download" android:name=".download.DownloadProvider"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="com.android.launcher.permission.INSTALL_SHORTCUT"/>
    <uses-permission android:name="android.permission.RESTART_PACKAGES"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.INSTALL_PACKAGES"/>
    <uses-permission android:name="android.permission.CLEAR_APP_CACHE"/>
    <uses-permission android:name="android.permission.DELETE_PACKAGES"/>
    <uses-permission android:name="android.permission.GET_PACKAGE_SIZE"/>
    <uses-permission android:name="android.permission.WRITE_APN_SETTINGS"/>
</manifest>