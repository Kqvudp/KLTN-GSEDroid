            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="Corpse finder(Samsung)" android:name="SDMstartpage" android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:label="Explorer" android:name="SDMexplorer" android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:label="Searcher" android:name="SDMsearch" android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:label="Corpse finder(Android)" android:name="SDMcorpsefinder" android:theme="@android:style/Theme.NoTitleBar"/>
