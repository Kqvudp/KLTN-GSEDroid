<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.alarm.puzle">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name=".Globals">
        <activity android:label="@string/app_name" android:name="Dashboard" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/add_alarm" android:name=".AddAlarm" android:screenOrientation="portrait"/>
        <activity android:label="@string/edit_alarm" android:name=".EditAlarm" android:screenOrientation="portrait"/>
        <activity android:name="EasyLevelPuzzel" android:screenOrientation="portrait" android:theme="@android:style/Theme.Dialog"/>
