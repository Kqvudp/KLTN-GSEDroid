        <activity android:name="MediumLevelPuzzel" android:screenOrientation="portrait" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name="HardLevelPuzzel" android:screenOrientation="portrait" android:theme="@android:style/Theme.Dialog"/>
        <activity android:name="EasyLevelSeries" android:screenOrientation="portrait" android:theme="@android:style/Theme.Dialog"/>
